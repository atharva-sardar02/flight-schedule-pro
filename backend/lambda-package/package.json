{
  "name": "flight-schedule-pro-backend",
  "version": "0.1.0",
  "description": "Backend Lambda functions for Flight Schedule Pro",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "dev": "ts-node-dev --respawn --transpile-only src/dev-server.ts",
    "dev:build": "npm run build && npm run watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.693.0",
    "@aws-sdk/client-secrets-manager": "^3.693.0",
    "@aws-sdk/client-ses": "^3.693.0",
    "@langchain/core": "^1.0.3",
    "@langchain/langgraph": "^1.0.1",
    "@langchain/openai": "^1.0.0",
    "axios": "^1.7.7",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^17.2.3",
    "joi": "^17.13.3",
    "langchain": "^0.3.5",
    "pg": "^8.13.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "@types/pg": "^8.11.10",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "aws-sdk-client-mock": "^4.1.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "express": "^4.21.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
